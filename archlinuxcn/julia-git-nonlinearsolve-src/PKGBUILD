package() {
  cd NonlinearSolve.jl

  rm -rf lib/

  JULIA_INSTALL_SRCPKG=1 . /usr/lib/julia/julia-install-pkg.sh NonlinearSolve "${pkgdir}" "${pkgname}" julia-git
}
