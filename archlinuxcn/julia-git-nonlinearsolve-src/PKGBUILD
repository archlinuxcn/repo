pkgname=julia-git-nonlinearsolve-src
pkgver=4.15.1
_commit=f264ba6eacc2f9acb49f7b27660b795eae3445eb
pkgrel=1
pkgdesc="NonlinearSolve.jl"
url="https://github.com/SciML/NonlinearSolve.jl.git"
arch=('any')
license=('MIT')
makedepends=(git julia-pkg-scripts)
