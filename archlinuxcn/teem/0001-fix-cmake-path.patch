+     in mnmfS <minScl>-<#smp>-<maxScl>[-<flags>] */
+  if ( (parmS = strchr(str, '/')) ) {
+    /* there are in fact parms */
+    *parmS = '\0';
+    parmS++;
+  } else {
+    parmS = NULL;
+  }
+  mnmfS = str;
+  if (!( 3 == airStrntok(mnmfS, "-") || 4 == airStrntok(mnmfS, "-") )) {
+    biffAddf(GAGE, "%s: didn't get 3 or 4 \"-\"-separated tokens in \"%s\"",
