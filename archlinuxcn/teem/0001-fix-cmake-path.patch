+                       char **extraParmsP,
+                       const char *_str) {
+  static const char me[]="gageStackBlurParmParse";
+  char *str, *mnmfS, *stok, *slast=NULL, *parmS, *eps;
+  int flagSeen[256];
+  double sigmaMin, sigmaMax, dggsm;
