+                    me, pval);
+          airMopError(mop); return 1;
+        }
+      } else if (strcpy(xeq, "v=") && strstr(stok, xeq) == stok) {
+        pval = stok + strlen(xeq);
+        if (1 != sscanf(pval, "%d", &verbose)) {
+          biffAddf(GAGE, "%s: couldn't parse \"%s\" as verbose int", me, pval);
+          airMopError(mop); return 1;
+        }
