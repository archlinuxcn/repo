+  double *ppOwn;            /* point coords, we DO own buffer */
+  unsigned int num;         /* how many points */
+  int isLoop;               /* points form a loop: logical indices into coord
+                               array are . . . num-2, num-1, 0, 1, . . .
+                               and index 0 is effectively arbitrary */
+} limnPoints;
