pkgname=()
source=()
_url=https://ftp.mozilla.org/pub/firefox/releases/${pkgver}esr/linux-x86_64/xpi

for _lang in "${_languages[@]}"; do
  _locale=${_lang%% *}
  _pkgname=firefox-esr60-i18n-${_locale,,}

  pkgname+=($_pkgname)
  source+=("firefox-esr60-i18n-$pkgver-$_locale.xpi::$_url/$_locale.xpi")
  eval "package_$_pkgname() {
    _package $_lang
  }"
done

# Don't extract anything
noextract=(${source[@]%%::*})

_package() {
  pkgdesc="$2 language pack for Firefox ESR 60"
  depends=("firefox-esr60>=$pkgver")
  install -Dm644 firefox-esr60-i18n-$pkgver-$1.xpi \
    "$pkgdir/usr/lib/firefox/browser/extensions/langpack-$1@firefox.mozilla.org.xpi"
}

