pkgname=julia-git-manifolds-src
pkgver=0.11.13
_commit=6c0b98449da806149e96a83a006dee99f41d588e
pkgrel=1
pkgdesc="Manifolds.jl"
url="https://github.com/JuliaManifolds/Manifolds.jl.git"
arch=('any')
