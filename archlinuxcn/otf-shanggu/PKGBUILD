prepare() {
    for i in ${noextract[@]%.7z}; do
        mkdir ${i#*-Shanggu}
        bsdtar -C ${i#*-Shanggu} -xf "$srcdir/$i.7z"
    done
}

package_ttf-shanggu-sans() {
    install -Dm644 -t "$pkgdir/usr/share/fonts/TTF/$pkgname" ShangguSans/ShangguSans-*.ttf
    install -Dm644 -t "$pkgdir/usr/share/licenses/$pkgname" ShangguSans/LICENSE.txt
}
package_otf-shanggu-sans() {
    install -Dm644 -t "$pkgdir/usr/share/fonts/OTF/$pkgname" ShangguSans/ShangguSans-*.otf
    install -Dm644 -t "$pkgdir/usr/share/licenses/$pkgname" ShangguSans/LICENSE.txt
}
