pkgname=xml2-config-compat
pkgver=0.1
pkgrel=1
pkgdesc="xml2-config script for compatibility"
url='https://gitlab.archlinux.org/archlinux/packaging/packages/libxml2/-/issues/3'
arch=('any')
source=(xml2-config)
sha256sums=('06db66874095c734cbea28236c4e7341f47dc08191bea60a4dba7f0562c5f16e')
license=("MIT")

package() {
	install -D -m755 "${srcdir}/xml2-config" "${pkgdir}/usr/bin/xml2-config"
}


