pkgname=julia-git-aws_checksums_jll-src
pkgver=0.2.10+0
_commit=5917fe1cbaebb7f5dce6451ba0d8e007082c49f0
pkgrel=1
pkgdesc="aws_checksums_jll.jl"
url="https://github.com/JuliaBinaryWrappers/aws_checksums_jll.jl.git"
arch=('any')
license=('MIT')
makedepends=(git julia-pkg-scripts)
depends=(julia-git aws-checksums)
source=("git+https://github.com/JuliaBinaryWrappers/aws_checksums_jll.jl.git#commit=$_commit"
        jll.toml)
sha256sums=('58bd868a40a1ea585ea2f0afa38989aeb7709eef09a652237140ae73ecc9085b'
            '633a6adecccef3a9d01ab8ed1e1634e57b120dcb62c1eeb54d43db2f17fff8bb')

build() {
  cd aws_checksums_jll.jl

  julia /usr/lib/julia/julia-gen-jll.jl aws_checksums_jll ../jll.toml
}

package() {
  cd aws_checksums_jll.jl

  JULIA_INSTALL_SRCPKG=1 . /usr/lib/julia/julia-install-pkg.sh aws_checksums_jll "${pkgdir}" "${pkgname}" julia-git
}
