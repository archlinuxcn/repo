            hash {
                algo = "sha1";
            };
        };
    };
    configurations {
        default = "conf-snow";
        conf-snow {
            kernel = "kernel";
            fdt = "fdt-snow";
        };
        conf-snow-rev5 {
            kernel = "kernel";
            fdt = "fdt-snow-rev5";
        };
        conf-spring {
            kernel = "kernel";
            fdt = "fdt-spring";
        };
        conf-peach-pi {
            kernel = "kernel";
            fdt = "fdt-peach-pi";
        };
        conf-peach-pit {
            kernel = "kernel";
            fdt = "fdt-peach-pit";
        };
        conf-veyron-brain {
            kernel = "kernel";
            fdt = "fdt-veyron-brain";
        };
        conf-veyron-jaq {
            kernel = "kernel";
            fdt = "fdt-veyron-jaq";
        };
        conf-veyron-jerry {
            kernel = "kernel";
            fdt = "fdt-veryon-jerry";
        };
        conf-veyron-mickey {
            kernel = "kernel";
            fdt = "fdt-veyron-mickey";
        };
        conf-veyron-minnie {
            kernel = "kernel";
            fdt = "fdt-veyron-minnie";
        };
        conf-veyron-pinky {
            kernel = "kernel";
            fdt = "fdt-veyron-pinky";
        };
        conf-veyron-speedy {
            kernel = "kernel";
            fdt = "fdt-veyron-speedy";
        };
        conf-nyan-big {
            kernel = "kernel";
            fdt = "fdt-nyan-big";
        };
        conf-nyan-blaze {
            kernel = "kernel";
