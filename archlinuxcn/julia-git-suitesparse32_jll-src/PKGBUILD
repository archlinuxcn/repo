pkgname=julia-git-suitesparse32_jll-src
pkgver=7.12.1+0
_commit=a6fb2e9a32f71312c7c06660e199439f8c727e40
pkgrel=1
pkgdesc="SuiteSparse32_jll.jl"
url="https://github.com/JuliaBinaryWrappers/SuiteSparse32_jll.jl.git"
arch=('any')
license=('MIT')
makedepends=(git julia-pkg-scripts)
depends=(julia-git suitesparse)
source=("git+https://github.com/JuliaBinaryWrappers/SuiteSparse32_jll.jl.git#commit=$_commit"
        jll.toml)
sha256sums=('a002c58d93e2371ed30f03125765f195507327646f289b14161c288a91745097'
            '1c57a8f14522ff97df83b36a05beca864e8e21832c0c083b031cc4afad2495db')

build() {
  cd SuiteSparse32_jll.jl

  julia /usr/lib/julia/julia-gen-jll.jl SuiteSparse32_jll ../jll.toml
}

package() {
  cd SuiteSparse32_jll.jl

  JULIA_INSTALL_SRCPKG=1 . /usr/lib/julia/julia-install-pkg.sh SuiteSparse32_jll "${pkgdir}" "${pkgname}" julia-git
}
