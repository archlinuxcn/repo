pkgname=julia-git-openssl-src
pkgver=1.6.0
_commit=7582295370acd3bb7466c805860a677df9ac0ba5
pkgrel=1
pkgdesc="OpenSSL.jl"
url="https://github.com/JuliaWeb/OpenSSL.jl.git"
arch=('any')
license=('MIT')
makedepends=(git julia-pkg-scripts)
depends=(julia-git)
