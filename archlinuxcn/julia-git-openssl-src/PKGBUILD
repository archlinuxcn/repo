sha256sums=('bc327f74d95f364bdfac1d15066c264807700407cb3fb6420ecf9c1f9fa346cd')

package() {
  cd OpenSSL.jl

  JULIA_INSTALL_SRCPKG=1 . /usr/lib/julia/julia-install-pkg.sh OpenSSL "${pkgdir}" "${pkgname}" julia-git
}
