pkgname=julia-git-aws_c_cal_jll-src
pkgver=0.9.13+0
_commit=4981b1255d29c7517a68ccb7b947d4c658fbd553
pkgrel=1
pkgdesc="aws_c_cal_jll.jl"
url="https://github.com/JuliaBinaryWrappers/aws_c_cal_jll.jl.git"
arch=('any')
license=('MIT')
makedepends=(git julia-pkg-scripts)
depends=(julia-git aws-c-cal)
source=("git+https://github.com/JuliaBinaryWrappers/aws_c_cal_jll.jl.git#commit=$_commit"
        jll.toml)
sha256sums=('f44da13ee70091ce3358e476f533284971400f1dcafd6eaad4f32fc9345bcb52'
            '1af6039f95dbdfe75b38b9ce07970aac68a6c1802829debf3b4c18b4bba355db')

build() {
  cd aws_c_cal_jll.jl

  julia /usr/lib/julia/julia-gen-jll.jl aws_c_cal_jll ../jll.toml
}

package() {
  cd aws_c_cal_jll.jl

  JULIA_INSTALL_SRCPKG=1 . /usr/lib/julia/julia-install-pkg.sh aws_c_cal_jll "${pkgdir}" "${pkgname}" julia-git
}
