From 7789e66a8aecf98cc254676208fa2ae684407148 Mon Sep 17 00:00:00 2001
From: Yichao Yu <yyc1992@gmail.com>
Date: Fri, 15 Aug 2025 12:24:38 -0400
Subject: [PATCH 07/11] [julia-git] Better support for loading packages from
 stdlib

---
 base/loading.jl | 4 ++++
 1 file changed, 4 insertions(+)

diff --git a/base/loading.jl b/base/loading.jl
index c846a781af..21a6f8057d 100644
--- a/base/loading.jl
+++ b/base/loading.jl
@@ -1255,6 +1255,10 @@ function explicit_manifest_entry_load_spec(manifest_file::String, pkg::PkgId, en
             ispath(path) && return PkgLoadSpec(entry_path(abspath(path), pkg.name, entryfile), syntax_version)
         end
     end
+    mbypath = manifest_uuid_path(Sys.STDLIB, pkg)
+    if mbypath isa String && isfile(mbypath)
+        return mbypath
+    end
     # no depot contains the package, return missing to stop looking
     return missing
 end
