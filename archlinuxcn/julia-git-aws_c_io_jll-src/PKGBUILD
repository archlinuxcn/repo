pkgname=julia-git-aws_c_io_jll-src
pkgver=0.26.1+0
_commit=faf16033feb16556d945b850815bdea6382a463f
pkgrel=1
pkgdesc="aws_c_io_jll.jl"
url="https://github.com/JuliaBinaryWrappers/aws_c_io_jll.jl.git"
arch=('any')
license=('MIT')
makedepends=(git julia-pkg-scripts)
depends=(julia-git aws-c-io)
source=("git+https://github.com/JuliaBinaryWrappers/aws_c_io_jll.jl.git#commit=$_commit"
        jll.toml)
sha256sums=('6a6a3c5adb023a7f81fe285220612f175b2d5d53ae4dbe193c6809522ee871fd'
            '549c70955f35c35c70166a442bc634bb2217af8967809704755988eac7567547')

build() {
  cd aws_c_io_jll.jl

  julia /usr/lib/julia/julia-gen-jll.jl aws_c_io_jll ../jll.toml
}

package() {
  cd aws_c_io_jll.jl

  JULIA_INSTALL_SRCPKG=1 . /usr/lib/julia/julia-install-pkg.sh aws_c_io_jll "${pkgdir}" "${pkgname}" julia-git
}
