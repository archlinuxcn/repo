arch=('any')
license=('GPL')
depends=()
optdepends=()
makedepends=()
replaces=()
conflicts=('texlive-bin' $(pacman -Sgq texlive-most texlive-lang))
provides=('texlive-bin' $(pacman -Sgq texlive-most texlive-lang))

build() {
   echo "Nothing to build"
}

package() {
   echo "Nothing to package"
}
