pkgname=julia-git-manifoldsbase-src
pkgver=0.15.15
_commit=0e45f73b1f35ff4fa4ee34967495eab11e3e2902
pkgrel=1
pkgdesc="ManifoldsBase.jl"
url="https://github.com/JuliaManifolds/ManifoldsBase.jl.git"
arch=('any')
license=('MIT')
makedepends=(git julia-pkg-scripts)
depends=(julia-git)
