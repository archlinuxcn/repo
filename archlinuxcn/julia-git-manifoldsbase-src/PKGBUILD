sha256sums=('SKIP')

package() {
  cd ManifoldsBase.jl

  JULIA_INSTALL_SRCPKG=1 . /usr/lib/julia/julia-install-pkg.sh ManifoldsBase "${pkgdir}" "${pkgname}" julia-git
}
