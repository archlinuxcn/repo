pkgname=julia-git-s2n_tls_jll-src
pkgver=1.6.2+0
_commit=4b0e91342ac32e01a4fb001002f4df21feb92870
pkgrel=1
pkgdesc="s2n_tls_jll.jl"
url="https://github.com/JuliaBinaryWrappers/s2n_tls_jll.jl.git"
arch=('any')
license=('MIT')
makedepends=(git julia-pkg-scripts)
depends=(julia-git s2n-tls)
source=("git+https://github.com/JuliaBinaryWrappers/s2n_tls_jll.jl.git#commit=$_commit"
        jll.toml)
sha256sums=('dc17e0d02863d80868d0b21ce608642cfd061a698429500e22e8767c17cdff24'
            '1740397b192b01a431132f41ebdfd704f2cc464f445246a1368c60a2362bedcc')

build() {
  cd s2n_tls_jll.jl

  julia /usr/lib/julia/julia-gen-jll.jl s2n_tls_jll ../jll.toml
}

package() {
  cd s2n_tls_jll.jl

  JULIA_INSTALL_SRCPKG=1 . /usr/lib/julia/julia-install-pkg.sh s2n_tls_jll "${pkgdir}" "${pkgname}" julia-git
}
