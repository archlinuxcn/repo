  depends=(
    'linux-asahi' 'uboot-asahi' 'm1n1' 'asahi-scripts' 'asahi-alarm-keyring' 'asahi-fwextract'
    'alsa-ucm-conf-asahi' 'asahi-configs' 'tiny-dfr'
  )
}

