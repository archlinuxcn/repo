post_install() {
  echo ">>> Qtile Example Config at"
  echo ">>> /usr/share/doc/qtile/default_config.py"
}

post_upgrade() {
  echo ">>> Qtile Example Config at"
  echo ">>> /usr/share/doc/qtile/default_config.py"
  echo ">>> WARNING: Some config breakage on 0.10.2, look at changelog https://github.com/qtile/qtile/blob/v0.10.2/CHANGELOG#L27"
}

