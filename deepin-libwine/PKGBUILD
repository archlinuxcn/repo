pkgname=deepin-libwine
pkgvers=2.18-16~rc2
pkgver=2.18_16
pkgrel=1
epoch=
pkgdesc="Deepin Libwine"
arch=('i686' 'x86_64')
url="http://www.deepin.org"
license=('Proprietary')
groups=()
depends=()
makedepends=('tar')
checkdepends=()
optdepends=()
provides=()
conflicts=()
replaces=()
backup=()
options=(!strip)
install=
changelog=
source=("https://mirrors.ustc.edu.cn/deepin/pool/non-free/d/deepin-wine/${pkgname}_${pkgvers}_i386.deb")
noextract=("${pkgname}_${pkgvers}_i386.deb")
md5sums=('3e9687db126acccfe282fe11c4ff3c2c')
validpgpkeys=()

prepare() {
	ar -x ${pkgname}_${pkgvers}_i386.deb
	mkdir ${pkgname}-${pkgvers}
	tar -xf data.tar.xz --directory="${pkgname}-${pkgvers}"
}

package() {
	cd "${pkgname}-${pkgvers}"
	cp -rf ./ ${pkgdir}/
}
