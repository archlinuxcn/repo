pkgname=aurvote
pkgver=1.7
pkgrel=1
pkgdesc="Tool to vote for favorite AUR packages"
url="http://git.archlinux.fr/aurvote.git/" 
license="GPL" 
arch=('any')
depends=('curl') 
source=($pkgname) 

package() { 
  	install -D -m 755 "$srcdir/$pkgname" "$pkgdir/usr/bin/$pkgname"
}

md5sums=('0cd536fff64f337e8ffe15e39fef95a5')
